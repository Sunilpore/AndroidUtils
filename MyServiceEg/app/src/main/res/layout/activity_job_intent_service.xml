<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="16dp"
    tools:context=".job_intent_service.JobIntentServiceActivity">

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edt_txt_ip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Type here"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_enque"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ENQUE BUTTON"
        android:onClick="enqueueWork"
        app:layout_constraintTop_toBottomOf="@id/edt_txt_ip"/>


</androidx.constraintlayout.widget.ConstraintLayout>